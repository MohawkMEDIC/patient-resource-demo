@model IEnumerable<Health.Models.SearchViewModel>
@{
    ViewBag.Title = "Medications";
}

<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
<script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>

<head>
<title>

</title>
</head>
<body>
@foreach (var item in Model)
{
    <div class="page-header row">
        <img src="http://bygghjalphemma.se/wp-content/uploads/2014/11/bill-gates-wealthiest-person.jpg" style="float:left; padding:0px 20px 0px 0px" height="175" width="160" class="img-circle"><h1>Medications</h1>
        <h4>@Html.DisplayFor(modelItem => item.FirstName) @Html.DisplayFor(modelItem => item.LastName)</h4>
        <h4>@Html.DisplayFor(modelItem => item.Identifier)</h4>
    </div>
    <br/>

        <div class="row">
            <div class="panel panel-danger">
                <div class="panel-heading">
                    <h3 class="panel-title">Prescriptions</h3>
                </div>
                <div class="panel-body">
                    <p> Medication: @Html.DisplayFor(modelItem => item.Medications)</p>
                    <p> Dosage: @Html.DisplayFor(modelItem => item.MedicationsAdmin)</p>
                </div>
            </div>
        </div>
    @Html.ActionLink(" ", "Index", "Patient", new { ID = item.Identifier }, new
   {
       style = "background: url('" + Url.Content("https://www.moneysmart.gov.au/media/399935/buying-a-home.png") + "') no-repeat; display: block; height:300px; width:300px;"
   })
}
</body>


